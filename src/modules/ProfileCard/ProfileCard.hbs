{{# Card cardClassName='card-profile'}}
  {{{ Avatar avatarType='card-profile' path=this.avatarPath }}}
  {{# Title level='3'}}
    {{ this.currentUserName }}
  {{/Title}}
  {{#each profileItems}}
    {{{ ProfileItem
       profileInfoTitle=this.profileInfoTitle
       profileInfoText=this.profileInfoText
    }}}
  {{/each}}
  <div class="card-profile__controls">
    {{# Button type='primary' onClick=this.goProfileEditPage }}
      Изменить данные
    {{/Button}}
    {{# Button type='primary' onClick=this.goProfileEditPasswordPage }}
      Изменить пароль
    {{/Button}}
  </div>
{{/Card}}
